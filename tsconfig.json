{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "paths": {
    "@/components/*": ["components/*"],
    "@/assets/*": ["assets/*"],
    "@/hooks/*": ["hooks/*"],
    "@/constant/*": ["constant/*"],
    "@/interfaces/*": ["interfaces/*"],
    "@/pages/*": ["pages/*"],
    "@/store/*": ["store/*"],
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/general/AppDrawer/AppNavDrawerItem.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
