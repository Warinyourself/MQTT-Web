{
  "extends": [
    "next",
    "airbnb",
    "prettier/react",
    "next/core-web-vitals",
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["react", "jsx-a11y", "prettier"],
  "rules": {
    "prettier/prettier": 1,
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0,
    "indent": ["error", 2],
    "linebreak-style": 1,
    "quotes": ["error", "double"]
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
